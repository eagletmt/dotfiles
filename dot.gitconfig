[user]
	name = Kohei Suzuki
	email = eagletmt@gmail.com
	signingkey = C48DBD97
[push]
	default = simple
[color]
	ui = auto
[grep]
	lineNumber = true
[log]
	date = iso
	decorate = full
[github]
	user = eagletmt
[alias]
	fetch-pulls = fetch origin +refs/pull/*:refs/remotes/pull/*
[include]
	path = .gitconfig.local
[harrow]
	disabled = true
[commit]
	gpgsign = true
[tag]
	forceSignAnnotated = true
[init]
	defaultBranch = main
