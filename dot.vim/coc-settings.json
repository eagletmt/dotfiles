{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    },
    "jsonnet": {
      "command": "jsonnet-language-server",
      "filetypes": ["jsonnet"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform"]
    }
  },
  "suggest.noselect": true,
  "inlayHint.enable": false,
  "rust-analyzer.server.path": "~/bin/rust-analyzer.sh"
}
